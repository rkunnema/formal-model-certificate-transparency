                                                                 <TR><TD ALIGN="LEFT" VALIGN="TOP"><FONT COLOR="#000000">SI1</FONT></TD> <TD ALIGN="LEFT" VALIGN="TOP">=</TD> <TD ALIGN="LEFT" VALIGN="TOP">sign(&lt;$idCA, $id, pkID, 'pubKey', ~sn, $i, $j, <BR ALIGN="LEFT"/> &#32;&#32;&#32;&#32;&#32;&lt;$idL, 'SCT', $sct, SI3&gt;, sct2&gt;,<BR ALIGN="LEFT"/> &#32;&#32;&#32;&#32;~skCA.1)</TD></TR></TABLE>>];

