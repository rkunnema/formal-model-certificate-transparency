   !TLS_L_to_M( $idL, $idM, $snapshot, sth, session ),
   !Root_CA( $rootCA, pkRootCA )
   ]
  --[
  Eq( verify(cert_sig, <$idCA, $subj, ke, 'pre-cert', sn, $i, $j>, pkCA),
