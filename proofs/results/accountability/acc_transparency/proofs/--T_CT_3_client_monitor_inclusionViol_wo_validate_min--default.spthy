rule (modulo E) ClientInitiatesAuditing:
   [
   In( ca_fields ), In( ca_sig ), In( pub_fields ), In( pub_sig ), In( sct )
   ]
  -->
   [
   !TLS_to_M( $idM, <<pub_fields, pub_sig>, ca_fields, ca_sig>, sct ),
   Client_Receipt_Val( $idM, <<pub_fields, pub_sig>, ca_fields, ca_sig> )
   ]

