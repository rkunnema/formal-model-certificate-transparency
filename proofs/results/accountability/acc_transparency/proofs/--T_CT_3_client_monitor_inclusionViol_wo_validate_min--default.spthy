rule (modulo E) MonitorStoresSnapshot:
   [
   !Logger( $idL, pkL ), !Monitor( $idM ),
   !TLS_L_to_M( $idL, $idM, $snapshot, sth, session )
   ]
  --[
  SnapshotStored( $idM, $idL, session, sth ),
  MonitorSnapshotStored( $idM, $idL, session, sth ), Time( $now )
  ]->
   [ ]

  /* has exactly the trivial AC variant */

rule (modulo E) ClientStoresSnapshot:
   [
   !Logger( $idL, pkL ), !TLS_L_to_C( $idL, $idC, $snapshot, sth, session )
   ]
  --[
  SnapshotStored( $idC, $idL, session, sth ),
  ClientSnapshotStored( $idC, $idL, session, sth ), Time( $now )
  ]->
   [ ]

  /* has exactly the trivial AC variant */

rule (modulo E) ClientFindsAppendOnlyViolation[color=#e65208]:
   [
   !Logger( $idL, pkL ), !TLS_L_to_C( $idL, $idC, $snapshot, sth, session )
   ]
  --[
  ClientFindsAppendOnlyViolation( $idL ),
  AppendOnlyCheck( $idC, sth, session ),
  AppendOnlyViolation( $idC, $idL, sth, session ), Time( $now )
  ]->
   [ ]

  /* has exactly the trivial AC variant */

predicate: EntryMissing( Logger, preC_fields, log_id, #time, sth_time )<=>¬(∃ different_time chain #t.
   ((LoggerComputesAddLog( Logger, log_id, preC_fields, chain,
                           different_time
     ) @ #t) ∧
    (¬(sth_time = different_time))) ∧
   (#t < #time))

predicate: EntryVisible( Logger, preC_fields, log_id, #time, sth_time )<=>∃ different_time chain #t.
 ((LoggerComputesAddLog( Logger, log_id, preC_fields, chain,
                         different_time
   ) @ #t) ∧
  (¬(sth_time = different_time))) ∧
 (#t < #time)

restriction AppendOnlyFails:
  "∀ sth session id IdL #t6.
    (AppendOnlyViolation( id, IdL, sth, session ) @ #t6) ⇒
    (∃ preC_fields chain time t log_id1 log_id2 time2 sth2 session2 #t0 #t1
       #t2 #t3 #t4 #t5.
