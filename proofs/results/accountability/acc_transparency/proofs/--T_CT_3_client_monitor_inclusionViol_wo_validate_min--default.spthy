   [
   !Root_CA( $rootCA, pkRootCA ),
   In( <$rootCA, $idCA, pkCA, 'pubKey-CA', snCA, $i0, $j0> ), In( ca_sig ),
   In( <$idCA, $id, pkID, 'pubKey', sn, $i, $j, 
        <$idL1, 'SCT', $sct1, sct1_sig>, $idL2, 'SCT', $sct2, sct2_sig>
   ),
   In( pub_sig ), !Logger( $idL1, pkL1 ), !Logger( $idL2, pkL2 )
