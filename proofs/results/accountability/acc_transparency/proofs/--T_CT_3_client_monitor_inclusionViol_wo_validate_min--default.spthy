rule (modulo E) Start_Log[color=#08a358]:
   [ !Logger_s( $idL, ~skL, pk(~skL) ) ]
  --[
  StartedLog( $idL, $log_id ), HonestStartedLog( $idL, $log_id ),
  Time( $now )
  ]->
