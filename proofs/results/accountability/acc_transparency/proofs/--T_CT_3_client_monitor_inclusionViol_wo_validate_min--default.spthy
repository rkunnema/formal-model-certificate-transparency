restriction rootCA_CA_distinct:
  "∀ rootCA idCA #t1 #t2.
    ((Root_CA_Init( rootCA ) @ #t1) ∧ (NonRoot_CA_Init( idCA ) @ #t2)) ⇒
    (¬(rootCA = idCA))"
  // safety formula

restriction rootCA_Logger_distinct:
  "∀ rootCA idL #t1 #t2.
    ((Root_CA_Init( rootCA ) @ #t1) ∧ (Logger_Registered( idL ) @ #t2)) ⇒
    (¬(rootCA = idL))"
  // safety formula

restriction RootCA_Monitor_distinct:
  "∀ rootCA idM #t1 #t2.
    ((Root_CA_Init( rootCA ) @ #t1) ∧ (Monitor_Registered( idM ) @ #t2)) ⇒
    (¬(rootCA = idM))"
  // safety formula

restriction CA_Logger_distinct:
  "∀ idCA idL #t1 #t2.
    ((NonRoot_CA_Init( idCA ) @ #t1) ∧ (Logger_Registered( idL ) @ #t2)) ⇒
    (¬(idCA = idL))"
  // safety formula

restriction CA_Monitor_distinct:
  "∀ idCA idM #t1 #t2.
    ((NonRoot_CA_Init( idCA ) @ #t1) ∧ (Monitor_Registered( idM ) @ #t2)) ⇒
    (¬(idCA = idM))"
  // safety formula

restriction Logger_Monitor_distinct:
  "∀ idL idM #t1 #t2.
    ((Logger_Registered( idL ) @ #t1) ∧ (Monitor_Registered( idM ) @ #t2)) ⇒
    (¬(idL = idM))"
  // safety formula

restriction LoggersUnique:
  "∀ idL #t1 #t2.
    ((Logger_Registered( idL ) @ #t1) ∧ (Logger_Registered( idL ) @ #t2)) ⇒
    (#t1 = #t2)"
  // safety formula

restriction CAsUnique:
  "∀ IdCA pk1 sk1 pk2 sk2 #t1 #t2.
    ((CA_Registered( IdCA, sk1, pk1 ) @ #t1) ∧
     (CA_Registered( IdCA, sk2, pk2 ) @ #t2)) ⇒
    (#t1 = #t2)"
  // safety formula

restriction eq:
  "∀ e1 e2 #t. (Eq( e1, e2 ) @ #t) ⇒ (e1 = e2)"
  // safety formula

restriction time_monotonicity:
  "∀ t #t1 #t3.
    (((Time( t ) @ #t1) ∧ (Time( t ) @ #t3)) ∧ (#t1 < #t3)) ⇒
    (∀ tp #t2.
      (((Time( tp ) @ #t2) ∧ (#t1 < #t2)) ∧ (#t2 < #t3)) ⇒ (tp = t))"
  // safety formula

restriction certStillValid:
  "∀ currTime valid_till #t1.
    ((Time( currTime ) @ #t1) ∧
     (CheckValidUntil( currTime, valid_till ) @ #t1)) ⇒
    (¬(∃ #t2. (#t2 < #t1) ∧ (Time( valid_till ) @ #t2)))"
  // safety formula

