predicate: ChainVisible( Logger, chain, log_id, #time, sth_time )<=>∃ preC_fields different_time #t.
 ((LoggerComputesAddLog( Logger, log_id, preC_fields, chain,
                         different_time
   ) @ #t) ∧
  (¬(sth_time = different_time))) ∧
 (#t < #time)

predicate: NoChainVisible( Logger, stmt, log_id, #time, sth_time )<=>(¬(∃ different_time #t.
    ((LoggerComputesAddLogStmt( Logger, log_id, stmt, different_time
      ) @ #t) ∧
     (¬(sth_time = different_time))) ∧
    (#t < #time))) ∨
((∀ preC_fields different_time stmt.1 chain #t2 #t.
   (((((LoggerComputesAddLogStmt( Logger, log_id, stmt.1,
                                  different_time
        ) @ #t) ∧
       (LoggerComputesAddLog( Logger, log_id, preC_fields, chain,
                              different_time
        ) @ #t)) ∧
      (¬(sth_time = different_time))) ∧
     (#t < #time)) ∧
    (ChainCheck( chain ) @ #t2)) ⇒
   (ChainInvalid( chain ) @ #t2)) ∧
 (∀ different_time preC_fields chain #t.
   ((LoggerComputesAddLog( Logger, log_id, preC_fields, chain,
                           different_time
     ) @ #t) ∧
    (#t < #time)) ⇒
   (∃ #t2. ChainCheck( chain ) @ #t2)))

restriction logExplorationSucceeds:
  "∀ IdM IdL chain sth session #t5.
    (MonitorObservesEntry( IdM, IdL, chain, sth, session ) @ #t5) ⇒
    (∃ log_id time #t2.
      ((LoggerPresentsView( IdL, IdM, log_id, time, sth, session ) @ #t2) ∧
       (#t2 < #t5)) ∧
      (∃ preC_fields different_time #t.
        ((LoggerComputesAddLog( IdL, log_id, preC_fields, chain, different_time
          ) @ #t) ∧
         (¬(time = different_time))) ∧
        (#t < #t2)))"

restriction logExplorationFails:
  "∀ IdM IdL stmt sth session #t1.
    (NothingObserved( IdM, IdL, stmt, sth, session ) @ #t1) ⇒
    (∃ log_id time #t0.
      ((LoggerPresentsView( IdL, IdM, log_id, time, sth, session ) @ #t0) ∧
       (#t0 < #t1)) ∧
      ((¬(∃ different_time #t.
           ((LoggerComputesAddLogStmt( IdL, log_id, stmt, different_time ) @ #t) ∧
            (¬(time = different_time))) ∧
           (#t < #t0))) ∨
       ((∀ preC_fields different_time stmt.1 chain #t2 #t.
          (((((LoggerComputesAddLogStmt( IdL, log_id, stmt.1, different_time
               ) @ #t) ∧
              (LoggerComputesAddLog( IdL, log_id, preC_fields, chain, different_time
               ) @ #t)) ∧
             (¬(time = different_time))) ∧
            (#t < #t0)) ∧
           (ChainCheck( chain ) @ #t2)) ⇒
          (ChainInvalid( chain ) @ #t2)) ∧
        (∀ different_time preC_fields chain #t.
          ((LoggerComputesAddLog( IdL, log_id, preC_fields, chain, different_time
            ) @ #t) ∧
           (#t < #t0)) ⇒
          (∃ #t2. ChainCheck( chain ) @ #t2)))))"

