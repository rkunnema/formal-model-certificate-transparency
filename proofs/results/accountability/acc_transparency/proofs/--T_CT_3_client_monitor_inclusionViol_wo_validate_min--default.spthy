  /* has exactly the trivial AC variant */

restriction honestStartLogOnlyOnce:
  "∀ IdL lid1 lid2 #t1 #t2.
    ((HonestStartedLog( IdL, lid1 ) @ #t1) ∧
     (HonestStartedLog( IdL, lid2 ) @ #t2)) ⇒
    (#t1 = #t2)"
  // safety formula

restriction uniqueLogId:
  "∀ IdL log_id #t1 #t2.
    ((StartedLog( IdL, log_id ) @ #t1) ∧ (StartedLog( IdL, log_id ) @ #t2)) ⇒
    (#t1 = #t2)"
  // safety formula

