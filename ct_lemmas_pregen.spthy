        AuditAuthCheck($idL, pre_fields, ca_fields, audit_stmt),
        AuditAuthCheck_noTrust($idL, ca_fields, audit_stmt),
        Time($now)
    ]->
    [ CraftReceipt($idM, chain)   ]

